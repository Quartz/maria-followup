drop_na_columns <- function(df) {
remove_cols <- c()
for(colname in colnames(df)) {
if(sum(is.na(df[,colname])) == nrow(df)) {
remove_cols <- c(remove_cols, colname)
}
}
df[,!(names(df) %in% remove_cols)]
}
